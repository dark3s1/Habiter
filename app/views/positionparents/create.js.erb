<% if @status %>
$("#realsIndex_<%= @positionparent.start_time.strftime("%Y-%m-%d") %>").append("<%= j(render "reals/real", real: @positionparent) %>");

<% else %>
$("#modalArea").html("<%= j(render 'form', habit: @habit) %>")
$("#modalArea").find(".modal").modal('show')

<% end %>

$("#realModal").modal('hide');
$('body').removeClass('modal-open');
$('.modal-backdrop').remove();
