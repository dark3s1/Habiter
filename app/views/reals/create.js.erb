<% if @status %>
$("#realsIndex_<%= @real.start_time.strftime("%Y-%m-%d") %>").append("<%= j(render "real", real: @real, b_real: @b_real) %>");

<% else %>
$("#modalArea").html("<%= j(render 'reals/form', habit: @habit) %>")
$("#modalArea").find(".modal").modal('show')

<% end %>

$("#realModal").modal('hide');
$('body').removeClass('modal-open');
$('.modal-backdrop').remove();
